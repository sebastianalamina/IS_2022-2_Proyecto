generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuario {
  email       String? @db.VarChar(255)
  contrasegna String? @db.VarChar(255)
  token       String? @db.VarChar(80)
  id          BigInt  @id @default(autoincrement())
}
